<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javasm.sys.mapper.CrmPermissionMapper">


    <select id="selectPermission" resultType="java.util.List">
        select id, p_name from crm_permission where role_id = #{role_id}
    </select>
</mapper>
